<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:zfaStandTypen="http://www.zfa.drv-bund.de/zfa_standardtypen/v01" xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.zfa.drv-bund.de/zfa_standardtypen/v01" elementFormDefault="qualified" version="04.00">
	<!-- Version 04.00 zum 01.01.2019 -->
	<!-- 	Technische Aenderungen -->
	<!-- 		Datei in UTF-8 codiert -->
	<!--		Encoding in XML Definition auf UTF-8 gesetzt -->
	<!--		String.Latin Standard von KoSIT in Version 1.1.1 fuer alle Stringtypen eingebunden -->
	<!-- Version 03.01 zum 01.01.2017 -->
	<!-- 	Aenderungen Versionierung -->
	<!-- 		LeistungsJahrMZ01V01Type auf das Maximaljahr 2015 angepasst -->
	<!-- 		BeitragJahrMZ10V02Type auf das Maximaljahr 2015 angepasst -->
	<!-- 		BeitragJahrMZ20V01Type auf das Maximaljahr 2015 angepasst -->
	<!-- Version 02.01 (S15) -->
	<!-- 	Fachliche Aenderungen -->
	<!-- 		UeStPostfachType auf maximal 8 Stellen erweitert -->
	<!-- 		DatumJHJJ-MM-2020Type hinzugefügt -->
	<!-- 		elster_BaseStringSType hinzugefügt -->
	<!-- 		elster_BaseStringNoLeadingOrTrailingSeparators hinzugefügt -->
	<!-- 		elster_BaseStringNoLeadingSeparators hinzugefügt -->
	<!-- 		elster_BaseStringUltraLightSType hinzugefügt -->
	<!-- 		LeistungsJahrMZ01V01Type hinzugefügt -->
	<!-- 		BeitragJahrMZ10V01Type hinzugefügt -->
	<!-- 		BeitragJahrMZ10V02Type hinzugefügt -->
	<!-- 		BeitragJahrMZ20V01Type hinzugefügt -->
	<!-- 		Ordnungsmerkmal_RBMSType hinzugefügt -->
	<!-- 		AuslPLZType nutzt jetzt den Typ elster_BaseStringSType -->
	<!-- 		AdressErgType nutzt jetzt den Typ elster_BaseStringSType -->
	<!-- 		Änderung im StaatSchlüsselType: KAP VERDE geändert auf CABO VERDE  -->
	<!-- 		Änderung im StaatSchlüsselType: BESET.PALAEST.GEBIETE geändert auf PALAEST.GEBIETE  -->
	<!-- Version 01.01 (S12) -->
	<!-- 	Aenderungen Versionierung -->
	<!-- 		Neuer Namespace für Versionierung -->
	<!--		Umbenennung in ZfAStandardtypenV01.xsd -->
	<!-- 		Erstellung von Elementen zur Verwendung in den MZ XSDs -->
	<!--			Inlandsadresse zfaStandTypen:InlandsadresseType -->
	<!--			Auslandsadresse zfaStandTypen:AuslandsadresseType -->
	<!--			MeldegrundDaten zfaStandTypen:MeldegrundDatenType -->
	<!--			FehlerDaten zfaStandTypen:FehlerDatenFachlichType -->
	<!--			UebermittelndeStelleDaten zfaStandTypen:UebermittelndeStelleDatenType -->
	<!--			MitteilungspflichtigenDaten zfaStandTypen:MipfDatenType -->
	<!-- 	Fachliche Aenderungen -->
	<!--		Austauschen der StaatSchluesselType -->
	<!--		DatumMit0Type, DatumMit0TagType Anpassung der Dokumentation Geburtsdatuemer zu Geburtsdaten -->
	<!-- 		IdNrType Beginn jetzt auch 1,2 und 3 zulaessig -->
	<annotation>
		<documentation>Einschraenkung auf lateinische Zeichen von KoSIT in Version 1.1.1</documentation>
	</annotation>
	<simpleType name="String.Latin">
		<restriction base="string">
			<pattern value="(([&#x9;-&#xa;&#xd;&#x20;-&#x7e;&#xa1;-&#xac;&#xae;-&#x107;&#x10a;-&#x11b;&#x11e;-&#x123;&#x126;-&#x131;&#x134;-&#x15b;&#x15e;-&#x16b;&#x16e;-&#x17e;&#x18f;&#x1a0;-&#x1a1;&#x1af;-&#x1b0;&#x1b7;&#x1cd;-&#x1d4;&#x1de;-&#x1df;&#x1e4;-&#x1f0;&#x1f4;-&#x1f5;&#x1fa;-&#x1ff;&#x218;-&#x21b;&#x21e;-&#x21f;&#x22a;-&#x22b;&#x22e;-&#x233;&#x259;&#x292;&#x1e02;-&#x1e03;&#x1e0a;-&#x1e0b;&#x1e10;-&#x1e11;&#x1e1e;-&#x1e21;&#x1e24;-&#x1e27;&#x1e30;-&#x1e31;&#x1e40;-&#x1e41;&#x1e44;-&#x1e45;&#x1e56;-&#x1e57;&#x1e60;-&#x1e63;&#x1e6a;-&#x1e6b;&#x1e80;-&#x1e85;&#x1e8c;-&#x1e93;&#x1e9e;&#x1ea0;-&#x1ea7;&#x1eaa;-&#x1eac;&#x1eae;-&#x1ec1;&#x1ec4;-&#x1ed3;&#x1ed6;-&#x1edd;&#x1ee4;-&#x1ef9;&#x20ac;])|(&#x4d;&#x302;|&#x4e;&#x302;|&#x6d;&#x302;|&#x6e;&#x302;|&#x44;&#x302;|&#x64;&#x302;|&#x4a;&#x30c;|&#x4c;&#x302;|&#x6c;&#x302;))*"/>
		</restriction>
	</simpleType>
	<annotation>
		<documentation>Basis-Type fuer Werte, die den folgenden Bedingungen genuegen:
						Beliebig viele Wiederholungen von Zeichen aus den Klassen
						{L} (Buchstaben)
						{P} (Satzzeichen)
						{Zs} (Leerzeichen)
						{Nd} (Dezimalzahlen), einschließlich keinmal.
						Zusaetzlich gelten die Einschraenkungen des BaseStringLightSTypes</documentation>
	</annotation>
	<simpleType name="BaseStringSType">
		<restriction base="zfaStandTypen:BaseStringLightSType">
			<pattern value="[\p{L}\p{P}\p{Zs}\p{Nd}]*"/>
		</restriction>
	</simpleType>
	<annotation>
		<documentation>Basis-Type fuer Werte, die den folgenden Bedingungen genuegen:
						kein Zeilenumbruch, kein Zeilenvorschub,
						mind ein Zeichen aus der Klasse {N} (Zahlen) oder {L} (Buchstaben).
						Zusaetzlich gelten die Einschraenkungen des BaseStringUltraLightSTypes</documentation>
	</annotation>
	<simpleType name="BaseStringLightSType">
		<restriction base="zfaStandTypen:BaseStringUltraLightSType">
			<pattern value=".*[\p{N}\p{L}].*"/>
		</restriction>
	</simpleType>
	<annotation>
		<documentation>Basis-Type fuer Werte, die den folgenden Bedingungen genuegen:
						kein Zeilenvorschub, kein Zeilenumbruch, kein Tabulator,
						kein Zeichen aus der Klasse {C} (andere Zeichen (die kein Buchstaben, Symbole, Zahlen oder Trennzeichen sind)),
						kein Zeichen aus der Klasse {Zl} (Zeilenwechsel)</documentation>
	</annotation>
	<simpleType name="BaseStringUltraLightSType">
		<restriction base="zfaStandTypen:String.Latin">
			<pattern value="([^\n\r\t\p{C}\p{Zl}])*"/>
		</restriction>
	</simpleType>
	<simpleType name="elster_BaseStringSType">
		<restriction base="zfaStandTypen:elster_BaseStringNoLeadingOrTrailingSeparators">
			<pattern value="[\p{L}\p{P}\p{Zs}\p{Nd}]*"/>
		</restriction>
	</simpleType>
	<simpleType name="elster_BaseStringNoLeadingOrTrailingSeparators">
		<restriction base="zfaStandTypen:elster_BaseStringNoLeadingSeparators">
			<pattern value=".*[\P{Z}]"/>
		</restriction>
	</simpleType>
	<simpleType name="elster_BaseStringNoLeadingSeparators">
		<restriction base="zfaStandTypen:elster_BaseStringUltraLightSType">
			<pattern value="[\P{Z}].*"/>
		</restriction>
	</simpleType>
	<simpleType name="elster_BaseStringUltraLightSType">
		<restriction base="zfaStandTypen:String.Latin">
			<pattern value="([^\n\r\t\p{C}\p{Zl}])*"/>
		</restriction>
	</simpleType>
	<simpleType name="Ordnungsmerkmal_RBMSType">
		<restriction base="zfaStandTypen:elster_BaseStringNoLeadingOrTrailingSeparators">
			<maxLength value="40"/>
			<minLength value="1"/>
			<pattern value="[\p{L}\p{P}\p{Zs}\p{Nd}&lt;&gt;|+]([\p{L}\p{P}\p{Zs}\p{Nd}&lt;&gt;|~|\-|+]*[\p{L}\p{P}\p{Zs}\p{Nd}&lt;&gt;|+])?"/>
		</restriction>
	</simpleType>
	<annotation>
		<documentation>Basis-Type fuer Werte, die den folgenden Bedingungen genuegen:
						kein Zeichen aus der Klasse {Sc} = "$¢£¥", 
						kein Zeichen aus der Klasse {No} = "²³¹",
						kein Zeichen aus der Klasse {Sk} = "^`¯", 
						kein Zeichen aus der Klasse {So} = "§©®°", 
						kein Zeichen aus der Klasse {Sm} = "+=|~±×÷" + gt + lt</documentation>
	</annotation>
	<simpleType name="BaseStringAnNameType">
		<restriction base="zfaStandTypen:String.Latin">
			<pattern value="([\p{L}|\p{P}|\p{Zs}|\p{Nd}])*"/>
		</restriction>
	</simpleType>
	<simpleType name="MmMeldType">
		<restriction base="unsignedByte">
			<enumeration value="1"/>
			<enumeration value="2"/>
			<enumeration value="9"/>
			<!-- 1 = Mitteilung -->
			<!-- 2 = Berichtigung -->
			<!-- 9 = Stornierung -->
		</restriction>
	</simpleType>
	<simpleType name="AnEmailType">
		<restriction base="zfaStandTypen:String.Latin">
			<maxLength value="128"/>
		</restriction>
	</simpleType>
	<simpleType name="DatumJHJJT-MM-TTType">
		<restriction base="zfaStandTypen:String.Latin">
			<length value="10"/>
			<pattern value="20[0-9]{2}-(12|11|10|0[1-9])-((3)(0|1)|(1|2)[0-9]|(0)[1-9])"/>
			<!-- jhjj-mm-tt -->
		</restriction>
	</simpleType>
	<simpleType name="DatumJHJJType">
		<restriction base="gYear">
			<minInclusive value="2010"/>
			<maxInclusive value="2099"/>
			<!-- Jahr des Zuflusses/Abflusses -->
			<!-- jhjj -->
		</restriction>
	</simpleType>
	<simpleType name="DatumJHJJ-MMType">
		<restriction base="zfaStandTypen:String.Latin">
			<length value="7"/>
			<pattern value="(19|20)[0-9]{2}-(12|11|10|0[1-9])"/>
		</restriction>
	</simpleType>
	<simpleType name="DatumJHJJ-MM-2020Type">
		<restriction base="zfaStandTypen:String.Latin">
			<length value="7"/>
			<pattern value="((19)[0-9]{2}|(20)[01][0-9]|(2020))-(12|11|10|0[1-9])"/>
		</restriction>
	</simpleType>
